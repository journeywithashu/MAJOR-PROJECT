<% layout('/layouts/boilerplate') %>
<body>
     <h3>All Listings </h3>
     <form action="/listings/new" method="GET">
          <button>Create New Listings</button>
     </form>
     <ul>
<%for(let listings of allListings){ %>
     <li><a href="/listings/<%=listings._id%>"><%= listings.title %></a></li>
<%}%>
     </ul>
    
    
</body>